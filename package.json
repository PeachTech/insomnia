{
  "name": "sidecar",
  "private": true,
  "version": "1.0.0",
  "description": "Peach Sidecar",
  "cache": 1,
  "main": "main.js",
  "licence": "GNU AGPLv3",
  "homepage": "https://peach.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeachTech/insomnia"
  },
  "scripts": {
    "test:lint": "eslint \"**/*.js\" \"**/*.html\" \"**/*.json\"",
    "test:noisy": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --silent && open ./coverage/lcov-report/index.html",
    "test:watch": "cross-env NODE_ENV=test jest --silent --watch",
    "test:flow": "flow check",
    "test": "npm run test:flow && npm run test:lint && cross-env NODE_ENV=test jest --silent --maxWorkers 1",
    "start-hot": "npm run build-main && cross-env HOT=1 INSOMNIA_ENV=development electron app",
    "build-main": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.electron.babel.js",
    "hot-server": "webpack-dev-server --config ./webpack/webpack.config.development.babel.js",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "build:clean": "rimraf ./build/* && rimraf ./dist/* && mkdirp ./build",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.production.babel.js",
    "build:main": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.electron.babel.js",
    "build:copy": "cp -r ./app/package.json ./bin ./app/static ./app/icons/* ./build/",
    "build:copywin": "mkdirp ./build/bin && mkdirp ./build/static && cpx ./app/package.json ./build && cpy ./bin/* ./build/bin/ && cpy ./app/static/* ./build/static && cpy ./app/icons/* ./build/ ",
    "build:install": "cd build && npm install",
    "build": "npm run build:clean && npm run build:renderer && npm run build:main && npm run build:copy && npm run build:install",
    "build:win": "npm run build:clean && npm run build:renderer && npm run build:main && npm run build:copywin && npm run build:install",
    "rebuild": "electron-rebuild -f -w node-libcurl",
    "package:mac": "rm -rf dist/mac && node node_modules/electron-builder/out/cli/cli.js --publish=never --x64 --mac",
    "package:win": "rimraf dist/win* && node node_modules/electron-builder/out/cli/cli.js --publish=never -w --x64 --ia32",
    "package:linux": "rm -rf dist/linux && rm -f dist/*.AppImage && rm -f dist/*.deb && node node_modules/electron-builder/out/cli/cli.js --publish=never --x64 --linux snap",
    "package": "npm test && rm -rf dist/* && npm run package:mac && npm run package:win && npm run package:linux",
    "build-n-package": "npm run build && npm run package",
    "build-n-package:win": "npm run build:win && npm run package:win",
    "build-n-package:mac": "npm run build && npm run package:mac",
    "build-n-package:linux": "npm run build && npm run package:linux"
  },
  "jest": {
    "setupFiles": [
      "./__jest__/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|png)$": "<rootDir>/__mocks__/dummy.js",
      "^worker-loader!": "<rootDir>/__mocks__/dummy.js"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "rootDir": "app",
    "verbose": false,
    "resetMocks": true,
    "resetModules": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/*.min.js",
      "!**/main.development.js",
      "!**/static/**",
      "!**/ui/components/**",
      "!**/__fixtures__/**",
      "!**/__tests__/**",
      "!**/__jest__/**",
      "!**/__mocks__/**",
      "!**/node_modules/**"
    ]
  },
  "build": {
    "appId": "com.sidecar.app",
    "extraResources": [
      {
        "from": "./bin",
        "to": "./bin",
        "filter": "yarn-standalone.js"
      }
    ],
    "protocols": [
      {
        "name": "Peach Sidecar",
        "role": "Viewer",
        "schemes": [
          "sidecar"
        ]
      }
    ],
    "fileAssociations": [],
    "directories": {
      "app": "build",
      "output": "dist"
    },
    "win": {
      "target": [
        "squirrel"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://peach-insomnia.s3.amazonaws.com/images/icon.png",
      "msi": false
    },
    "linux": {
      "executableName": "peach sidecar",
      "synopsis": "A simple, beautiful, and free REST API client",
      "category": "Development",
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "identity": null
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3",
        "libcurl3"
      ]
    }
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "aws4": "^1.8.0",
    "classnames": "^2.2.6",
    "clone": "^2.1.2",
    "codemirror": "^5.43.0",
    "codemirror-graphql": "^0.6.12",
    "copy": "^0.3.2",
    "copy-dir": "^0.3.0",
    "deep-equal": "^1.0.1",
    "electron-builder-squirrel-windows": "^20.38.5",
    "electron-context-menu": "^0.9.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "^0.13.2",
    "hawk": "^7.0.10",
    "highlight.js": "^9.13.1",
    "hkdf": "^0.0.2",
    "html-entities": "^1.2.0",
    "iconv-lite": "^0.4.24",
    "insomnia-httpsnippet": "^1.16.19",
    "insomnia-importers": "^1.5.6",
    "insomnia-node-libcurl": "^1.2.28",
    "jsonlint": "^1.6.3",
    "jsonpath": "^0.2.12",
    "jwt-authentication": "^0.3.6",
    "marked": "^0.3.19",
    "mime-types": "^2.1.21",
    "mkdirp": "^0.5.1",
    "moment": "^2.23.0",
    "multiparty": "^4.2.1",
    "nedb": "^1.8.0",
    "node-abi": "^2.5.1",
    "node-forge": "^0.7.6",
    "nunjucks": "^3.1.7",
    "oauth-1.0a": "^2.2.5",
    "pdfjs-dist": "^1.10.100",
    "peachapisec": "^1.5.28",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-tabs": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "srp-js": "^0.2.1",
    "tar": "^3.1.7",
    "tough-cookie": "^2.5.0",
    "uuid": "^3.3.2",
    "vkbeautify": "^0.99.1",
    "whatwg-fetch": "^2.0.4",
    "xmldom": "^0.1.22",
    "xpath": "^0.0.23"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^2.0.0",
    "cpx": "^1.5.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.26.2",
    "electron": "^2.0.0",
    "electron-builder": "^20.27.1",
    "electron-rebuild": "^1.6.0",
    "eslint": "^3.16.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^2.1.1",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.58.0",
    "jest": "^19.0.2",
    "less": "^2.7.3",
    "less-loader": "^2.2.3",
    "ncp": "^2.0.0",
    "react-hot-loader": "^3.1.3",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.11.3",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
